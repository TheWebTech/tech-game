{
  "name": "tech-game",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && webpack --mode production",
    "deploy": "rm -rf dist && webpack --mode production && hs upload dist tech-game",
    "lint": "vue-cli-service lint",
    "prettier:write": "prettier --write 'src/**/*.js' 'src/**/*.json'",
    "serve": "vue-cli-service serve",
    "start": "rm -rf dist && webpack --watch --env.autoupload --mode development",
    "upload": "hs upload dist tech-game"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@hubspot/cms-cli": "^1.1.9",
    "@hubspot/webpack-cms-plugins": "^1.1.7",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^7.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^13.2.0",
    "vue-loader": "^17.0.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
